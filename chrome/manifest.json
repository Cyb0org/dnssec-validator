{
  "name": "DNSSEC Validator",
  "default_locale": "en",
  "version": "0.1",
  "description": "Shows DNSSEC status",
  "background_page": "background.html",
  "options_page": "options.html",
  "page_action" :
  {
    "default_icon" : "icon_unknown.png",
    "default_title" : "DNSSEC Validator",
    "popup": "popup.html"
  },
  "permissions" : [
    "tabs"
  ],
  "plugins": [
    { "path": "npDNSSECValidatorPlugin_x86.so" },
    { "path": "npDNSSECValidatorPlugin_x86-64.so" },
    { "path": "npDNSSECValidatorPlugin_win32.dll" },
    { "path": "DNSSECValidatorPlugin.plugin" }
  ],
  "icons" : {
    "32" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  }
}
