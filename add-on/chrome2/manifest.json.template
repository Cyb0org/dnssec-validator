      "popup": "popup.html"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "18",
   "permissions": [ "experimental", "tabs", "webRequest", "*://*/*" ],
   "plugins": [ {
      "path": "dnssecplugin"
