   "icons": {
      "128": "icon.png",
      "32": "icon.png",
      "48": "icon.png"
   },
   "name": "DNSSEC/TLSA Validator",
   "options_page": "options.html?4,0,8.8.8.8",
   "page_action": {
      "default_icon": "icon_action.gif",
